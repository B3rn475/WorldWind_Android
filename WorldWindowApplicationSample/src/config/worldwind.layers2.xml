        actuate="onLoad"
        className="gov.nasa.worldwind.layers.ScalebarLayer"
        title="Scale Bar" />
    <Layer
        actuate="onLoad"
        href="config/Earth/OpenStreetMapLayer.xml" />
    <Layer
        actuate="onLoad"
        className="gov.nasa.worldwind.layers.RenderableLayer"
        title="My Location Scale x1" >
        <Property
            name="MaxActiveAltitude"
            value="1e3" />
    </Layer>
    <Layer
        actuate="onLoad"
        className="gov.nasa.worldwind.layers.RenderableLayer"
        title="Image Location Scale x1" >
        <Property
            name="MaxActiveAltitude"
            value="1e3" />
    </Layer>
    <Layer
        actuate="onLoad"
